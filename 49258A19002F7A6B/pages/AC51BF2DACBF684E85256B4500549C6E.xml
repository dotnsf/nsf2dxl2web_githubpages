<pardef id="1" leftmargin="1in" hide="notes" tabs="L0.5000in L1in L1.5000in L2in L2.5000in L3in L3.5000in L4in"/>
<p def="1"><run html="true"><font color="white"/>&lt;script language="JavaScript"&gt;  </run></p>
<p def="1"><run html="true"><font color="white"/>function LoadOfflinePage()  </run></p>
<p def="1"><run html="true"><font color="white"/>{  </run></p>
<p def="1"><run html="true"><font color="white"/>url = location.href;</run></p>
<p def="1"><run html="true"><font color="white"/>StartOfPath = url.search(":") + 3;</run></p>
<p def="1"><run html="true"><font color="white"/>EndOfPath = url.search(".nsf") + 4;</run></p>
<p def="1"><run html="true"><font color="white"/>len = EndOfPath - StartOfPath;</run></p>
<p def="1"><run html="true"><font color="white"/>var Host = location.host;</run></p>
<p def="1"><run html="true"><font color="white"/></run></p>
<p def="1"><run html="true"><font color="white"/>PathAndDB = url.substr(StartOfPath, len);</run></p>
<p def="1"><run html="true"><font color="white"/>LastSlash = PathAndDB.lastIndexOf("/");</run></p>
<p def="1"><run html="true"><font color="white"/>FirstSlash = PathAndDB.indexOf("/");</run></p>
<p def="1"><run html="true"><font color="white"/>db = PathAndDB.substr(LastSlash + 1, PathAndDB.length - LastSlash);</run></p>
<p def="1"><run html="true"><font color="white"/></run></p>
<p def="1"><run html="true"><font color="white"/>docid = "</run><run html="true"><computedtext><code event="value"></code></computedtext></run><run html="true"><font color="white"/>";</run></p>
<p def="1"><run html="true"><font color="white"/></run></p>
<p def="1"><run html="true"><font color="white"/>if(FirstSlash != LastSlash)</run></p>
<p def="1"><run html="true"><font color="white"/>{</run></p>
<p def="1"><run html="true"><font color="white"/>path = PathAndDB.substr(FirstSlash + 1, LastSlash - FirstSlash - 1);</run></p>
<p def="1"><run html="true"><font color="white"/>path = path.replace(' ', '+');</run></p>
<p def="1"><run html="true"><font color="white"/>top.location = 'http://' + Host +'/' + path + '/' + db + '/$All/' + docid + '?OpenDocument';</run></p>
<p def="1"><run html="true"><font color="white"/>}</run></p>
<p def="1"><run html="true"><font color="white"/>else</run></p>
<p def="1"><run html="true"><font color="white"/>{</run></p>
<p def="1"><run html="true"><font color="white"/>top.location = 'http://' +Host  +'/' + db + '/$All/' + docid + '?OpenDocument';</run></p>
<p def="1"><run html="true"><font color="white"/>}</run></p>
<p def="1"><run html="true"><font color="white"/></run></p>
<p def="1"><run html="true"><font color="white"/>}</run></p>
<p def="1"><run html="true"><font color="white"/></run></p>
<p def="1"><run html="true"><font color="white"/>document.writeln('&lt;center&gt;&lt;a href="javascript:LoadOfflinePage();"&gt;&lt;img src="DOLS.gif" border="0"  width="33" height="33" alt="Click here to take this subscription offline"&gt;&lt;/a&gt;&lt;/center&gt;');</run></p>
<p def="1"><run html="true"><font color="white"/>&lt;/script&gt;</run></p></richtext></body>
